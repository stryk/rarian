$("#catalyst").append('<%= j render :partial =>'shared/catalysts', :locals => {:catalyst => @catalyst} %>');
$("#more-cat").spin(false);
$("#more-cat").html('More...');
$('#cat-pagination').replaceWith("<%= j will_paginate @catalyst, :param_name => 'cat_page', :id => 'cat-pagination'  %>");
$('#cat-pagination').hide();
url = $('#cat-pagination .next_page').attr('href');
if (!url) {
	$("#more-cat").html('');
}