<% Competitor.get_competitors(@company, 5).each do |competitor| %>
    (<%= competitor.no_of_votes %>) : <%= competitor.name %>
<% end %>
<br/>
<% if current_user %>
  <%= link_to "Add Competitor", "#", :id => 'company_competitor_slide' %>
  <%= render :partial => 'competitors/add_competitor' %>
<% end %>
