<% if current_user.up_voted?(@obj) %>
$("#vote_down_"+'<%= @obj.class.name.downcase+'_'+@obj.id.to_s %>').hide();
$("#vote_up_"+'<%= @obj.class.name.downcase+'_'+@obj.id.to_s %>').html('<i class="icon-thumbs-up"></i>Upvoted(undo)');
<% else %>
$("#vote_down_"+'<%= @obj.class.name.downcase+'_'+@obj.id.to_s %>').show();
$("#vote_up_"+'<%= @obj.class.name.downcase+'_'+@obj.id.to_s %>').html('<i class="icon-thumbs-up"></i>Upvote');

<% end %>
