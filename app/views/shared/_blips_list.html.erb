<% objs.each do |cobj| %>

  <%= render :partial => 'shared/voting_list', :locals => {:cobj => cobj, :company => company, :text_val => objs_text,
                                                           :child_objs_present => !child_objs.blank?,
                                                           :child_objs => child_objs,
                                                           :child_objs_text => child_objs_text,
                                                           :comments => true, :show_link => false, :refresh_div => false} %>



  <script type="text/javascript">
    $("#jTruncate_truncate-<%= cobj.id %>").jTruncate({
      length: 50
    });
  </script>
<% end %>
